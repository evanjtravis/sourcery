#!/usr/bin/bash

echo -e "\t${USR}/bashrc"

umask 022

#####################################################################
# Environment Variables
#####################################################################
LS_COLORS='di=96'
add_to_path ${HOME}/twee

export BASHINIT=${MYBASHLIB}/init
export PYINIT=${MYPYLIB}/init

add_to_path $BASHINIT
add_to_path /cygdrive/c/HashiCorp/Vagrant/bin

add_to_pythonpath $PYINIT

