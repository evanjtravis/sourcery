#!/usr/bin/bash

alias c="clear"
alias cscroll="printf '\033[3J'" # Clear terminal scroll buffer
alias djtest="python manage.py test"
alias ls="ls --group-directories-first --color=auto"
alias sourcery="source ${MYHOME}/.bash_profile"
